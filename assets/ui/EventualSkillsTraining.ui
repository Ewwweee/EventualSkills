{
  "type": "EventualSkills:EventualSkillsTraining",
  "skin": "EventualSkills:EventualSkills",
  "contents": {
    "type": "RowLayout",
    "contents": [
      {
        "type": "RelativeLayout",
        "contents": [
          {
            "type": "UIBox",
            "layoutInfo": {
              "position-top": {
                "target": "TOP",
                "widget": "mainContent",
                "offset": -10
              },
              "position-left": {
                "target": "LEFT",
                "widget": "mainContent",
                "offset": -10
              },
              "position-right": {
                "target": "RIGHT",
                "widget": "mainContent",
                "offset": -10
              },
              "position-bottom": {
                "target": "BOTTOM",
                "widget": "mainContent",
                "offset": -10
              }
            }
          },
          {
            "type": "MigLayout",
            "id": "mainContent",
            "colConstraints": "[100::200][10::200][min!]",
            "layoutInfo": {
              "use-content-width": true,
              "use-content-height": true,
              "position-vertical-center": {},
              "position-horizontal-center": {}
            },
            "layoutConstraints": "wrap 2, align left",
            "contents": [
              {
                "type": "ScrollableArea",
                "id": "scrollArea",
                "verticalScrollbar": true,
                "content": {
                  "type": "UIList",
                  "readOnly": true,
                  "multiline": true,
                  "id": "skillList",
                  "family": "skill-list"
                },
                "layoutInfo": {}
              },
              {
                "type": "MigLayout",
                "layoutInfo": {
                  "use-content-width": true,
                  "use-content-height": true,
                  "position-vertical-center": {},
                  "position-horizontal-center": {}
                },
                "rowConstraints": "[min!][10::20][10::40]",
                "layoutConstraints": "wrap 1, align left",
                "contents": [
                  {
                    "type": "UILabel",
                    "text": "Skill",
                    "layoutInfo": {}
                  },
                  {
                    "type": "UILabel",
                    "id": "selectedSkillName",
                    "layoutInfo": {}
                  },
                  {
                    "type": "UILabel",
                    "text": "Progress",
                    "layoutInfo": {}
                  },
                  {
                    "type": "UILabel",
                    "id": "selectedSkillProgress",
                    "layoutInfo": {}
                  },
                  {
                    "type": "UILabel",
                    "text": "",
                    "layoutInfo": {}
                  },
                  {
                    "type": "UIButton",
                    "id": "selectedSkillAction",
                    "layoutInfo": {}
                  },
                  {
                    "type": "UILabel",
                    "text": "Description"
                  },
                  {
                    "type": "UILabel",
                    "id": "selectedSkillDescription"
                  },
                  {
                    "type": "UILabel",
                    "text": "Required Skills",
                    "layoutInfo": {}
                  },
                  {
                    "type": "UILabel",
                    "id": "selectedSkillPrerequisiteSkills"
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  }
}
